<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/header"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="5dp"
		android:layout_marginRight="10dp"
		android:padding="8dp"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/imageview2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/header"
			android:scaleType="center"
			android:layout_gravity="center_horizontal|center_vertical" />
		<TextView
			android:id="@+id/headL"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="𝗩𝗶𝗲𝘄 &amp; 𝗖𝗹𝗲𝗮𝗻 𝗖𝘂𝘀𝘁𝗼𝗺𝗶𝘇𝗮𝗯𝗹𝗲 𝗙𝗶𝗹𝗲𝘀 , 𝗙𝗼𝗹𝗱𝗲𝗿𝘀 𝗼𝗿 𝗗𝗶𝗿𝗲𝗰𝘁𝗼𝗿𝗶𝗲𝘀"
			android:textSize="12sp"
			android:textColor="#000000" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="5dp"
		android:layout_marginTop="20dp"
		android:layout_marginRight="5dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="𝗣𝗮𝗿𝘁𝗶𝘁𝗶𝗼𝗻 :"
			android:textSize="12sp"
			android:textColor="#000000" />
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<RadioButton
				android:id="@+id/internal"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="Internal Storage"
				android:textSize="12sp"
				android:textColor="#000000" />
			<RadioButton
				android:id="@+id/userr"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="Multi-User"
				android:textSize="12sp"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/customPath"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="3dp"
				android:background="#FFCDD2"
				android:text=" Custom Path "
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#D50000" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear3"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="20dp"
		android:layout_marginRight="10dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="𝗧𝘆𝗽𝗲 : "
			android:textSize="12sp"
			android:textColor="#000000" />
		<HorizontalScrollView
			android:id="@+id/hscroll2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<RadioButton
					android:id="@+id/folder"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="14dp"
					android:padding="8dp"
					android:text="Directory"
					android:textSize="12sp"
					android:textColor="#000000" />
				<RadioButton
					android:id="@+id/file"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="File 𝐨𝐫 Extension"
					android:textSize="12sp"
					android:textColor="#000000" />
				<ImageView
					android:id="@+id/imageview1"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:src="@drawable/question"
					android:scaleType="fitCenter"
					android:layout_gravity="center_horizontal|center_vertical" />
			</LinearLayout>
		</HorizontalScrollView>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear5"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="20dp"
		android:layout_marginRight="10dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="𝗜𝗻𝗽𝘂𝘁 : "
			android:textSize="14sp"
			android:textColor="#000000" />
		<HorizontalScrollView
			android:id="@+id/hscroll3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<EditText
				android:id="@+id/smart"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:gravity="left|center_vertical"
				android:textSize="14sp"
				android:textColor="#000000"
				android:hint="Select Type                      "
				android:textColorHint="#607D8B"
				android:layout_gravity="left|center_vertical" />
		</HorizontalScrollView>
	</LinearLayout>
	<CheckBox
		android:id="@+id/sensitive"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="60dp"
		android:padding="8dp"
		android:text="Name Characters are insensitive"
		android:textSize="12sp"
		android:textColor="#000000" />
	<LinearLayout
		android:id="@+id/layooo"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="15dp"
		android:layout_marginRight="10dp"
		android:layout_marginBottom="2dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/view"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:layout_marginLeft="10dp"
			android:layout_marginTop="2dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="1dp"
			android:padding="8dp"
			android:background="#B9F6CA"
			android:gravity="center_horizontal|center_vertical"
			android:text="🔍 View"
			android:textSize="12sp"
			android:textColor="#000000"
			android:layout_weight="1" />
		<TextView
			android:id="@+id/clean"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:layout_marginLeft="5dp"
			android:layout_marginTop="2dp"
			android:layout_marginRight="10dp"
			android:layout_marginBottom="1dp"
			android:padding="8dp"
			android:background="#B9F6CA"
			android:gravity="center_horizontal|center_vertical"
			android:text="🚀 𝗖𝗹𝗲𝗮𝗻"
			android:textSize="12sp"
			android:textColor="#000000"
			android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear9"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="5dp"
		android:layout_marginRight="10dp"
		android:layout_marginBottom="2dp"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview17"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:layout_marginLeft="10dp"
			android:layout_marginTop="2dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="1dp"
			android:padding="8dp"
			android:background="#B9F6CA"
			android:gravity="center_horizontal|center_vertical"
			android:text="📂 Saved Items"
			android:textSize="12sp"
			android:textColor="#000000"
			android:layout_weight="1" />
		<TextView
			android:id="@+id/textview18"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:layout_marginLeft="5dp"
			android:layout_marginTop="2dp"
			android:layout_marginRight="10dp"
			android:layout_marginBottom="1dp"
			android:padding="8dp"
			android:background="#B9F6CA"
			android:gravity="center_horizontal|center_vertical"
			android:text="📂 Save"
			android:textSize="12sp"
			android:textColor="#000000"
			android:layout_weight="1" />
	</LinearLayout>
	<TextView
		android:id="@+id/textview4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="75dp"
		android:padding="8dp"
		android:text="𝗧𝗲𝗺𝗽𝗹𝗮𝘁𝗲's : "
		android:textSize="12sp"
		android:textColor="#000000" />
	<LinearLayout
		android:id="@+id/linear6"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
		<HorizontalScrollView
			android:id="@+id/hscroll4"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<LinearLayout
				android:id="@+id/linear7"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="apk"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="mkv"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="png"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="jpg"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="pdf"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview10"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="webm"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview11"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="7z"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="zip"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview15"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="gif"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview14"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="mp3"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
				<TextView
					android:id="@+id/textview13"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="1dp"
					android:padding="3dp"
					android:text="mp4"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#0D47A1" />
			</LinearLayout>
		</HorizontalScrollView>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear8"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="5dp"
		android:orientation="horizontal"
		android:layout_gravity="center_horizontal|center_vertical">
		<TextView
			android:id="@+id/textview16"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="1dp"
			android:padding="3dp"
			android:text="last Screenshot"
			android:textSize="14sp"
			android:textColor="#0D47A1"
			android:layout_gravity="center_horizontal|center_vertical" />
	</LinearLayout>
	<TextView
		android:id="@+id/result"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:text="result"
		android:textSize="10sp"
		android:textColor="#FFFFFF" />
</LinearLayout>
